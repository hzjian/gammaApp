UPDATE locations SET wkb_geometry = st_geometryfromtext('POINT('|| easting ||' '|| northing ||')',2770);

SELECT ST_AsGeoJSON(ST_Transform(
            ST_Buffer(wkb_geometry, 100,‘quad_segs=8‘),4326)) 
            AS geom, name
            FROM geodata.schools
            
            
CREATE TABLE lgdtemp(  
   ID  SERIAL PRIMARY KEY,  
   state  INT  NOT NULL,  
   geom geometry(Polygon,4326)  
);   
  
  
